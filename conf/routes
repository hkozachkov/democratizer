

GET         /ui                                 controllers.Application.ui

POST        /savebaseline                       controllers.REST.postBaseline
POST        /savebasevalues                     controllers.REST.postBaseValues

GET         /getuser/:profile                   controllers.REST.getUser(profile: String)
POST        /saveuser                           controllers.REST.postUser
POST        /anonymizeuser/:user                controllers.REST.anonymizeUser(user: Long)

GET         /getOverview/:user                  controllers.REST.getOverview(user: Long)
GET         /getVoteview/:user/:baseline        controllers.REST.getVoteview(user: Long, baseline: Long)
POST        /submit                             controllers.REST.postSubmission



